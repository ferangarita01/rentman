{
  "name": "@rentman/agent-gateway",
  "version": "1.0.0",
  "description": "AI Agent Gateway - Microservice for external AI assistants and autonomous agents",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "docker:build": "docker build -t rentman-agent-gateway .",
    "docker:run": "docker run -p 3001:3001 rentman-agent-gateway"
  },
  "keywords": [
    "ai-gateway",
    "openapi",
    "mcp",
    "fastify",
    "rentman"
  ],
  "author": "Rentman Team",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.0",
    "@fastify/rate-limit": "^10.0.0",
    "@fastify/swagger": "^9.0.0",
    "@fastify/swagger-ui": "^5.0.0",
    "@fastify/websocket": "^11.0.0",
    "@supabase/supabase-js": "^2.39.3",
    "fastify-type-provider-zod": "^4.0.0",
    "dotenv": "^16.4.1",
    "fastify": "^5.0.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "stripe": "^14.14.0",
    "tweetnacl": "^1.0.3",
    "zod": "^3.22.4",
    "redis": "^4.6.13"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.5",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}